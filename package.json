{
    "name": "mooship-vite-react-tailwind-starter",
    "private": true,
    "version": "0.0.1",
    "license": "MIT",
    "author": "Timothy Brits <brits@fastmail.com> (https://github.com/mooship)",
    "type": "module",
    "scripts": {
        "dev": "vite --open",
        "dev:host": "vite --open --host",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "npx eslint . --ext .ts,.tsx,.cjs",
        "lint:fix": "npx eslint . --ext .ts,.tsx,.cjs --fix",
        "format": "npx prettier . --write",
        "cleanup": "npm run lint:fix && npm run format"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@trivago/prettier-plugin-sort-imports": "^3.4.0",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "@typescript-eslint/eslint-plugin": "^5.43.0",
        "@typescript-eslint/parser": "^5.43.0",
        "@vitejs/plugin-react": "^2.2.0",
        "autoprefixer": "^10.4.13",
        "eslint": "^8.28.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.11",
        "eslint-plugin-react-hooks": "^4.6.0",
        "postcss": "^8.4.19",
        "postcss-import": "^15.0.0",
        "prettier": "^2.7.1",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.6.4",
        "vite": "^3.2.3"
    },
    "engines": {
        "node": ">=16",
        "npm": ">=8"
    }
}
